{
  "name": "Chisel client",
  "version": "1.0.2",
  "slug": "chisel_client",
  "description": "A TCP tunnel over ssh and a websocket to your public server to get remote access to your local home assistant instance",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "application",
  "boot": "auto",
  "host_network": true,
  "options": {
    "ha_host": "hassio.local",
    "ha_port": 8123,
    "chisel_server": "chisel.example.com",
    "chisel_remote_host": "127.0.0.1",
    "chisel_remote_port": 8123,
    "fingerprint": "00:00:00",
    "chisel_user": "",
    "chisel_password": "",
    "keepalive": "30s",
    "max-retry-count": 30
  },
  "schema": {
    "ha_host": "str",
    "ha_port": "port",
    "chisel_server": "str",
    "chisel_remote_host": "str",
    "chisel_remote_port": "port",
    "fingerprint": "str",
    "chisel_user": "str",
    "chisel_password": "str",
    "keepalive": "str",
    "max-retry-count": "int"
  }
}
